set fileencodings=utf-8,cp932                                                                                     

set number
set cursorline
set hlsearch
set incsearch
set smartindent
set clipboard+=unnamed
set laststatus=2
set wildmenu
 

syntax enable
nnoremap <Esc><Esc> :nohlsearch<CR>

nnoremap <C-j> }
nnoremap <C-k> {
